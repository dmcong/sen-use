{
  "name": "@sen-use/components",
  "version": "1.0.5",
  "author": "Michael",
  "repository": "git+https://github.com/dmcong/sen-use.git",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "copyfiles -s -e \"**/*.ts\" -e \"**/*.tsx\" -u 1 \"src/**/*\" \"dist/\" && tsc-alias",
    "clean": "rm -rf ./dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "engines": {
    "node": ">=16 <=18"
  },
  "resolutions": {
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.44.3",
    "react": "^17.0.39",
    "react-dom": "^17.0.11"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@solana/wallet-adapter-base": "^0.9.9",
    "@solana/wallet-adapter-react": "^0.15.8",
    "antd": "^4.19.1"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.44.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsc-alias": "^1.6.10",
    "typescript": "^4.5.2"
  }
}
